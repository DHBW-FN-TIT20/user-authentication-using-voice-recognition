{
    "python.analysis.typeCheckingMode": "basic",
    "notebook.compactView": true,
    "notebook.lineNumbers": "on",
    "notebook.outputFontFamily": "Monaco, 'Courier New', monospace",
    "notebook.outputFontSize": 14,
    "notebook.outputLineHeight": 1.3,
    "notebook.undoRedoPerCell": false,
    "notebook.output.textLineLimit": 100,

    // background color of notebook to black
    "workbench.colorCustomizations": {
        "[Atom One Dark]": {
            "notebook.editorBackground": "#1e2127"
        }
    }
}